
<div class="jumbotron">
  <div class="container">
    <h1>Welcome to the Csound Notebook!</h1>
    <p>Edit, render, and organize Csound notes, all online, all without having Csound installed.</p>
    <p><a class="btn btn-primary btn-lg" role="button" href="about.html">Learn more &raquo;</a></p>
  </div>
</div>

<div class="container">

  <h2>Latest Public Notes</h2>
  <% for note in @latest_public_notes %>
    <div>
      <h4><%= note.title %></h4>
      <h5>User: <%= note.user.username %></h5>
      <p>  <a href="/notejs/<%= note.id %>" target="_blank">View (Emscripten)</a> -
      <a href="/note/<%= note.id %>" target="_blank">View (PNaCl)</a></p>
    </div>
  <% end %>

</div>

